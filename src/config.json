{
  "name": "ZabbixTrigger",
  "type": "trigger",
  "execProgram": "node",
  "main": "app.js",
  "exposeRoute": true,
  "imgUrl": "zabbix.png",
  "version": "1.1.1",
  "description": "Handle zabbix webhooks request",
  "methods": [{
    "name": "webhook",
    "route": "post /zabbix/webhook",
    "viewName": "Zabbix Webhook",
    "params": [
      {
        "name": "SECRET",
        "viewName": "Secret",
        "type": "vault"
      }
      ,{
        "name": "SEVERITY",
        "viewName": "Severity Level",
        "type": "string"
      }
    ]
  }]
}